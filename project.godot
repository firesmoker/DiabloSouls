mouse_cursor/custom_image="res://assets/cursor.png"
window/canvas_textures/default_texture_filter=0

[editor_plugins]

enabled=PackedStringArray("res://addons/AS2P/plugin.cfg", "res://addons/markdown_book/plugin.cfg", "res://addons/markdownlabel/plugin.cfg")

